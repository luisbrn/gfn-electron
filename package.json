{
  "name": "geforcenow",
  "appId": "com.electron.${name}",
  "productName": "GeForce Now",
  "version": "1.0.2",
  "description": "A cross-platform web app for GeForce Now for ChromeBook",
  "main": "scripts/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "linux": {
      "target": "zip",
      "category": "Games",
      "artifactName": "${name}_${version}_linux.${ext}"
    },
    "extraFiles": [
      "geforcenow-electron.desktop"
    ]
  },
  "repository": "https://github.com/electron/geforcenow-electron",
  "keywords": [
    "Electron",
    "gfn",
    "geforcenow",
    "geforce-now"
  ],
  "author": "Hora»õiu Mlendea <Mlendea.Horatiu@GMail.com>",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^10.0.0-beta.23",
    "electron-builder": "^22.5.1"
  }
}
